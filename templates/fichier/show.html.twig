{% extends 'base.html.twig' %}

{% block title %}Fichier{% endblock %}

{% block body %}
    <h1>Fichier</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ fichier.id }}</td>
            </tr>
            <tr>
                <th>Nom_fichier</th>
                <td>{{ fichier.nomFichier }}</td>
            </tr>
            <tr>
                <th>Date_fichier</th>
                <td>{{ fichier.dateFichier ? fichier.dateFichier|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_fichier_index') }}">back to list</a>

    <a href="{{ path('app_fichier_edit', {'id': fichier.id}) }}">edit</a>

    {{ include('fichier/_delete_form.html.twig') }}
{% endblock %}
